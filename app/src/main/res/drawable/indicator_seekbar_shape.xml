<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background"
        android:bottom="5dp"
        android:top="5dp">
        <shape>
            <solid android:color="@color/layout_bg_ededed"/>
            <corners android:radius="50dp"/>
        </shape>
    </item>
    <item
        android:id="@android:id/progress"
        android:bottom="5dp"
        android:top="5dp">
        <clip>
            <shape>
                <solid android:color="@color/layout_bg_fcf749"/>
                <corners android:radius="50dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
